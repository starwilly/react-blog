{
  "name": "react-blog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^5.0.4",
    "formik": "^0.11.11",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "yup": "^0.25.1"
  },
  "lint-staged": {
    "src/**/*.{scss,css}": [
      "prettier --config .prettierrc --write",
      "git add"
